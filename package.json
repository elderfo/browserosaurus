{
  "name": "browserosaurus",
  "productName": "Browserosaurus",
  "version": "5.0.0",
  "engines": {
    "node": ">=10.4.1"
  },
  "description": "The browser prompter for macOS",
  "homepage": "http://wstone.io/browserosaurus/",
  "main": "src/main.ts",
  "scripts": {
    "docs": "http-server ./docs",
    "start": "ENV=DEV electron-forge start",
    "test": "jest",
    "lint": "tslint --project tsconfig.json --type-check --force",
    "prettier": "prettier --config .prettierrc --write 'src/**/*.js'",
    "package": "yarn run build-css && electron-forge package --platform=darwin --arch=x64",
    "make": "yarn run build-css && electron-forge make",
    "build-css": "node-sass-chokidar src/style -o build --importer=node_modules/node-sass-package-importer/dist/cli.js --output-style compressed",
    "watch-css": "node-sass-chokidar src/style -o build --importer=node_modules/node-sass-package-importer/dist/cli.js --output-style compressed --watch --recursive",
    "icns": "cd ./src/images/icon && ./png2icns.sh icon.png"
  },
  "repository": "https://github.com/will-stone/browserosaurus",
  "bugs": {
    "url": "https://github.com/will-stone/browserosaurus/issues"
  },
  "keywords": [
    "Electron",
    "Browser chooser"
  ],
  "author": "Will Stone",
  "license": "MIT",
  "devDependencies": {
    "@types/jsonpath": "^0.2.0",
    "@types/lodash": "^4.14.120",
    "@types/mousetrap": "^1.6.1",
    "@types/xml2js": "^0.4.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "http-server": "^0.11.1",
    "node-sass-chokidar": "^0.0.3",
    "node-sass-package-importer": "^5.1.0",
    "prettier": "^1.16.0",
    "react-test-renderer": "^16.4.1",
    "react-testing-library": "^5.4.4",
    "tslint": "^5.1.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "~2.2.2"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.0.1",
    "@types/electron-devtools-installer": "^2.0.2",
    "@types/electron-store": "^1.3.0",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "electron-compile": "^6.4.4",
    "electron-devtools-installer": "^2.1.0",
    "electron-store": "^2.0.0",
    "jsonpath": "^0.2.11",
    "lodash": "^4.17.4",
    "mousetrap": "^1.6.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-spring": "^5.4.0",
    "react-transition-group": "^2.4.0",
    "semver": "^5.4.1",
    "tslib": "^1.4.0",
    "xml2js": "^0.4.19"
  },
  "resolutions": {
    "eslint-scope": "3.7.1"
  },
  "config": {
    "forge": {
      "make_targets": {
        "darwin": [
          "dmg"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "icon": "src/images/icon/icon.icns",
        "ignore": [
          "docs",
          "src/style",
          "__mocks__",
          "node_modules",
          "StartBrowserosaurus.app",
          "StartBrowserosaurus.scpt"
        ],
        "protocols": [
          {
            "name": "HTTP link",
            "schemes": [
              "http",
              "https"
            ]
          },
          {
            "name": "File",
            "schemes": [
              "file"
            ]
          }
        ]
      }
    }
  }
}
