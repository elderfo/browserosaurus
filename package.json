{
  "name": "browserosaurus",
  "productName": "Browserosaurus",
  "version": "5.0.0",
  "engines": {
    "node": ">=10.4.1"
  },
  "description": "The browser prompter for macOS",
  "homepage": "http://wstone.io/browserosaurus/",
  "main": "src/main.ts",
  "scripts": {
    "docs": "http-server ./docs",
    "start": "ENV=DEV electron-forge start",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint --project tsconfig.json --type-check --force",
    "prettier": "prettier --config .prettierrc --write 'src/**/*.js'",
    "package": "yarn run build-css && electron-forge package --platform=darwin --arch=x64",
    "make": "yarn run build-css && electron-forge make",
    "build-css": "node-sass-chokidar src/picker/style.scss -o build --importer=node_modules/node-sass-package-importer/dist/cli.js --output-style compressed",
    "watch-css": "node-sass-chokidar src/picker/style.scss -o build --importer=node_modules/node-sass-package-importer/dist/cli.js --output-style compressed --watch --recursive",
    "icns": "cd ./src/images/icon && ./png2icns.sh icon.png"
  },
  "repository": "https://github.com/will-stone/browserosaurus",
  "bugs": {
    "url": "https://github.com/will-stone/browserosaurus/issues"
  },
  "keywords": [
    "Electron",
    "Browser chooser"
  ],
  "author": "Will Stone",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-3": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/electron-store": "^1.3.0",
    "@types/jest": "^23.3.13",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash": "^4.14.120",
    "@types/mousetrap": "^1.6.1",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/styled-components": "^4.1.11",
    "@types/xml2js": "^0.4.3",
    "babel-jest": "^23.0.0",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "http-server": "^0.11.1",
    "jest": "^24.0.0",
    "jest-dom": "^3.0.1",
    "node-sass-chokidar": "^0.0.3",
    "node-sass-package-importer": "^5.1.0",
    "prettier": "^1.16.4",
    "react-testing-library": "^5.4.4",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.0.1",
    "electron-compile": "^6.4.4",
    "electron-store": "^2.0.0",
    "jsonpath": "^0.2.11",
    "lodash": "^4.17.4",
    "mousetrap": "^1.6.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-spring": "^7.2.10",
    "react-transition-group": "^2.4.0",
    "semver": "^5.4.1",
    "styled-components": "^4.1.3",
    "tslib": "^1.4.0",
    "xml2js": "^0.4.19"
  },
  "config": {
    "forge": {
      "make_targets": {
        "darwin": [
          "dmg"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "icon": "src/images/icon/icon.icns",
        "ignore": [
          "docs",
          "src/picker/style.scss",
          "__mocks__",
          "node_modules",
          "StartBrowserosaurus.app",
          "StartBrowserosaurus.scpt"
        ],
        "protocols": [
          {
            "name": "HTTP link",
            "schemes": [
              "http",
              "https"
            ]
          },
          {
            "name": "File",
            "schemes": [
              "file"
            ]
          }
        ]
      }
    }
  }
}
